@model List<StackOverflowProject.ViewModels.QuestionViewModel>
@{
    ViewBag.Title = "Search";
}

<div class="row">
    <h3>Search results for @ViewBag.str</h3>
    <div class="col-sm-12">
        <table class="table">
            @foreach (var item in Model)
            {
                Html.RenderPartial("Question", item);
            }
        </table>
    </div>
</div>

<script>
    $("a[href='/allquestions']").attr("id", "active")
</script>


